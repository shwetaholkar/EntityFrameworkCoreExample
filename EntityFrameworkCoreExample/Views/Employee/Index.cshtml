@model List<Employee>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


<div class="container">
    <div class="row">
        <table class="table">

            <thead>
                <tr>
                    <th>Employee_Id</th>
                    <th>Employee_Name</th>
                    <th>Department_Name</th>
                    <th>Salary</th>
                    <th>Address</th>
                    <th>Job-Title</th>

                </tr>
            </thead>

            <tbody>
                @foreach (var emp in Model)
                {
                    <tr>
                        <td>@emp.Id</td>
                        <td>@emp.Name</td>
                        <td>@emp.Department.Name</td>
                        <td>@emp.Salary</td>
                        <td>@emp.Address</td>
                        <td>@emp.Job.Name</td>
                    </tr>

                }
            </tbody>

        </table>

    </div>


</div>